{
  "name": "ts-styled-components-plugin",
  "version": "1.0.1",
  "description": "TypeScript transformer plugin that adds minification support to styled-components",
  "keywords": [
    "typescript-plugin",
    "styled-components",
    "styled-components-plugin",
    "minifier"
  ],
  "author": "Lchemist",
  "license": "Apache-2.0",
  "homepage": "https://github.com/Lchemist/ts-styled-components-plugin",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lchemist/ts-styled-components-plugin.git"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "scripts": {
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "lint": "tsc --noEmit --isolatedModules && eslint --ext ts .",
    "prebuild": "rimraf tsconfig.tsbuildinfo lib",
    "build": "tsc -b",
    "release": "semantic-release"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=5.2"
  },
  "peerDependencies": {
    "ttypescript": ">=1",
    "typescript": ">=3"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/node": "^14.14.22",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "cz-cc": "^1.0.1",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^7.0.2",
    "lint-staged": "^10.5.4",
    "pinst": "^2.1.4",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.3.7",
    "typescript": "^4.1.5"
  }
}
